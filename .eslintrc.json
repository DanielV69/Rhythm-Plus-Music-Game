{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2017,
		"sourceType": "module",
		"babelOptions":{
			"configFile": ".babelrc"
		}
	},
	"plugins": [
		"prettier"
	],
	"extends": [
		"eslint:recommended",
		"airbnb",
		"prettier"
	],
	"settings": {
		"import/resolver": {
			"node": {
				"extensions": [
					".js"
				]
			}
		}
	},
	"env": {
		"es6": true,
		"browser": true
	}
}
